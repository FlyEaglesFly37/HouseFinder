{
  "name": "housefinder",
  "version": "1.0.0",
  "description": "House Finder is an application that will easily allow it's users to keep track of different homes while \"house hunting\".",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
