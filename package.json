  "bugs": {
    "url": "https://github.com/FlyEaglesFly37/HouseFinder/issues"
  },
  "homepage": "https://github.com/FlyEaglesFly37/HouseFinder#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "express": "^4.16.4",
    "mongoose": "^5.4.22",
    "react": "^16.8.6",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0"
  },
  "devDependencies": {
    "nodemon": "^1.18.10"
  }
}
